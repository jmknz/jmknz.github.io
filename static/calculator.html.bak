<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Calculator</title>
    <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="../css/calculator-style.css" rel="stylesheet" >
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="../js/calculator.js"></script>
  </head>
  <body>
    <div class="container-fluid text-center">
      <div class="calculator">
        <div class="text-center">
          <h1 id="title">BASIC CALCULATOR</h1>
        </div>
        <div class="output">
          <p id="entry">0</p>
          <p id="history">0</p>
        </div>
        <div class="keypad text-center">
          <button class="button btn" value="%">%</button>
          <button class="button btn" value="7">7</button>
          <button class="button btn" value="8">8</button>
          <button class="button btn" value="9">9</button>
          <button class="button btn" value="divide">&divide;</button>
          <button class="button btn" value="plusmn">&plusmn;</button>
          <button class="button btn" value="4">4</button>
          <button class="button btn" value="5">5</button>
          <button class="button btn" value="6">6</button>
          <button class="button btn" value="mult">x</button>
          <button class="button btn" value="clear">C</button>
          <button class="button btn" value="1">1</button>
          <button class="button btn" value="2">2</button>
          <button class="button btn" value="3">3</button>
          <button class="button btn" value="subtr">-</button>
          <button class="button btn" value="allClear">AC</button>
          <button class="button btn" value="0">0</button>
          <button class="button btn" value=".">.</button>
          <button class="button btn" value="equal">=</button>
          <button class="button btn" value="add">+</button>
        </div>
      </div>
    </div>
  </body>
</html>
